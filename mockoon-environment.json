{
  "uuid": "mock-env-railway",
  "name": "Mockoon API Railway",
  "port": 3000,
  "routes": [
    {
      "uuid": "route-get-users",
      "method": "get",
      "endpoint": "users",
      "responses": [
        {
          "uuid": "res-get-users",
          "body": "[{\"id\":1,\"name\":\"Alice\"},{\"id\":2,\"name\":\"Bob\"}]",
          "statusCode": 200,
          "headers":[{"key":"Content-Type","value":"application/json"}]
        }
      ]
    },
    {
      "uuid": "route-post-login",
      "method": "post",
      "endpoint": "login",
      "responses": [
        {
          "uuid": "res-post-login",
          "body": "{\"status\":\"ok\",\"token\":\"fake-token\"}",
          "statusCode": 200,
          "headers":[{"key":"Content-Type","value":"application/json"}]
        }
      ]
    }
  ],
  "cors": true
}
